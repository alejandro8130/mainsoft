<fieldset>
  <div class="row compact">
 	<div class="col-xs-2">
   	<strong>Fecha</strong>
 	</div>
 	<div class="col-xs-2">
   	<strong>Cantidad</strong>
 	</div>
 	<div class="col-xs-2">
   	<strong>Valor</strong>
 	</div>
  </div>
  <div class="row compact">
	<div class="col-xs-2">
	  
    <%= f.input :cantidad_producto %>
    <%= f.input :valor %>
    
    <%= f.association :producto %>
    <%= f.association :venta %>
	</div>
	<%= f.hidden_field :_destroy %>
	<%= link_to "Eliminar", '#', class: "remove_fields" %>
  </div>
</fieldset>
