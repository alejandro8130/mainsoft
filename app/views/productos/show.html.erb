<div class="col-md-offset-5">
  <p>
    <strong>Nombre:</strong>
    <%= @producto.nombre %>
  </p>
  
  <p>
    <strong>Marca:</strong>
    <%= @producto.marca %>
  </p>
  
  <p>
    <strong>Garantíaa:</strong>
    <%= @producto.garantia %>
  </p>
  
  <p>
    <strong>Proveedor:</strong>
    <%= @producto.proveedor.try(:nombre)  %>
  </p>
  
  <p>
    <strong>Cantidad:</strong>
    <%= @producto.cantidad %>
  </p>
  
  <p>
    <strong>Precio:</strong>
    <%= @producto.precio %>
  </p>
  
  <%= link_to 'Editar', edit_producto_path(@producto),class:"btn btn-info" %> |
  <%= link_to 'Atrás', productos_path, class:"btn btn-warning" %>
</div>